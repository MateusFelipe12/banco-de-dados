--
-- TOC entry 234 (class 1259 OID 17467)
-- Name: vw_aniversariantes; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_aniversariantes AS
 SELECT c.nome,
    c.data_nascimento,
    co.telefone,
    co.email
   FROM (public.clientes c
     JOIN public.contatos co ON ((c.id_contato = co.id)))
  WHERE ((EXTRACT(day FROM c.data_nascimento) = EXTRACT(day FROM now())) AND (EXTRACT(month FROM c.data_nascimento) = EXTRACT(month FROM now())));


ALTER TABLE public.vw_aniversariantes OWNER TO postgres;

--
-- TOC entry 232 (class 1259 OID 17457)
-- Name: vw_estoque_fornecedor; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_estoque_fornecedor AS
 SELECT p.id AS codigo,
    p.descricao,
    p.valor_custo AS custo,
    p.estoque,
    f.id AS matricula_fornecedor,
    f.nome AS fornecedor,
    c.telefone,
    c.email
   FROM ((public.pecas p
     JOIN public.fornecedores f ON ((p.id_fornecedor = f.id)))
     JOIN public.contatos c ON ((f.id_contato = c.id)));


ALTER TABLE public.vw_estoque_fornecedor OWNER TO postgres;

--
-- TOC entry 230 (class 1259 OID 17443)
-- Name: vw_informacoes_clientes; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_informacoes_clientes AS
 SELECT c.id,
    c.nome AS cliente,
    e.cidade,
    e.bairro,
    e.rua,
    e.numero,
    co.celular,
    co.telefone,
    co.email
   FROM (((public.clientes c
     JOIN public.enderecos_clientes ec ON ((c.id_endereco = ec.id)))
     JOIN public.enderecos e ON ((ec.id_enderecos = e.id)))
     JOIN public.contatos co ON ((c.id_contato = co.id)));


ALTER TABLE public.vw_informacoes_clientes OWNER TO postgres;

--
-- TOC entry 233 (class 1259 OID 17462)
-- Name: vw_pecas_clientes; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_pecas_clientes AS
 SELECT p.id AS codigo,
    p.descricao AS peca,
    p.estoque,
    s.descricao AS servico,
    to_char((s.data)::timestamp with time zone, 'DD/MM/YYYY'::text) AS realizacao,
    si.valor_total AS valor,
    v.placa,
    v.modelo,
    v.marca AS veiuculo,
    c.id AS cliente,
    c.nome
   FROM (((((public.pecas p
     JOIN public.pecas_servicos pc ON ((p.id = pc.id_pecas)))
     JOIN public.servico s ON ((pc.id_servico = s.id)))
     JOIN public.servicos_itens si ON ((s.id_servicos_itens = si.id)))
     JOIN public.veiculos v ON ((si.id_veiculo = v.id)))
     JOIN public.clientes c ON ((v.id_proprietario = c.id)));


ALTER TABLE public.vw_pecas_clientes OWNER TO postgres;

--
-- TOC entry 231 (class 1259 OID 17448)
-- Name: vw_priprietario_veiculos; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_priprietario_veiculos AS
 SELECT v.placa,
    v.marca,
    v.modelo,
    v.ano,
    c.nome AS proprietario,
    c.id AS cod_proprietario
   FROM (public.veiculos v
     JOIN public.clientes c ON ((v.id_proprietario = c.id)));


ALTER TABLE public.vw_priprietario_veiculos OWNER TO postgres;

--
-- TOC entry 229 (class 1259 OID 17438)
-- Name: vw_veiculo_servicos; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_veiculo_servicos AS
 SELECT v.placa,
    v.modelo AS veiculo,
    c.id,
    c.nome AS proprietario,
    s.descricao AS servico,
    p.descricao AS pecas,
    si.valor_total AS valor_servico
   FROM (((((public.servicos_itens si
     JOIN public.veiculos v ON ((si.id_veiculo = v.id)))
     JOIN public.clientes c ON ((v.id_proprietario = c.id)))
     JOIN public.servico s ON ((si.id = s.id_servicos_itens)))
     JOIN public.pecas_servicos pc ON ((s.id = pc.id_servico)))
     JOIN public.pecas p ON ((pc.id_pecas = p.id)));


ALTER TABLE public.vw_veiculo_servicos OWNER TO postgres;

